        "maxItems": 2
      }
    },
    "asks": {
      "type": "array",
      "items": {
        "type": "array",
        "items": [
          { "type": "number" },
          { "type": "number" }
        ],
        "minItems": 2,
        "maxItems": 2
      }
    }
  },
  "additionalProperties": true
}
